CD = cd
MAKE = make
RMV = rm -fv

PM_SUFFIX = .pm

all:
	$(CD) Util && $(MAKE) .discore-all$(PM_SUFFIX) all
	$(CD) Markup && $(MAKE) all
	$(CD) DOM && $(MAKE) all

clean:
	$(RMV) *.BAK .*.BAK *~ .*~
	$(CD) Body && $(MAKE) clean
	$(CD) Field && $(MAKE) clean
	$(CD) MIME && $(MAKE) clean
	$(CD) Util && $(MAKE) clean
	$(CD) Markup && $(MAKE) clean
	$(CD) DOM && $(MAKE) clean

